        -ms-grid-column 3
        -ms-grid-column-span 1
        grid-row 1
        grid-column 2
        width 100%
      .logo
        -ms-grid-row 3
        -ms-grid-column 1
        -ms-grid-column-span 3
        grid-row 2
        grid-column 1/-1
        width 100%
        &:before
          width 100%
        &_name
          margin 0
          font-size 40px
        &_type
          margin 18px 0 0
      .contacts
