  .main
    .flexslider
      .slider
        &_content
          &:before
            height 130px
          &__headline
            font-size 40px
    .welcome
      .three-blocks
        grid-template-columns 1fr
        -ms-grid-columns  1fr
        -ms-grid-rows auto w_gatter auto  w_gatter auto
        max-width 400px
        margin 0 auto
        &_item
          &--events
            -ms-grid-row 1
            -ms-grid-column 1
          &--food
            -ms-grid-row 3
            -ms-grid-column 1
          &--venues
            -ms-grid-row 5
            -ms-grid-column 1
    .introduction
      &_content
        display block
    .menu
      &-items
        grid-template-columns 1fr 1fr
        -ms-grid-columns 1fr w_gatter 1fr
        -ms-grid-rows auto w_gatter auto
        > div:nth-child(1)
          -ms-grid-row 1
          -ms-grid-column 1
        > div:nth-child(2)
          -ms-grid-row 1
          -ms-grid-column 3
        > div:nth-child(3)
          -ms-grid-row 3
          -ms-grid-column 1
        > div:nth-child(4)
          -ms-grid-row 3
          -ms-grid-column 3
    .book
      &_btn
        font-size 14px
        padding 17px 27px
    .reviews-slides
      &_content
        padding 20px 50px
        font-size 16px
    .blog
      &-items
        flex-direction column
        &_item
          margin 25px 0
    .gallery-block
      .gallery
        grid-template-columns 1fr 1fr
        grid-template-rows  250px 250px 250px
        -ms-grid-columns 1fr w_gatter 1fr
        -ms-grid-rows 250px w_gatter 250px w_gatter 250px
        &_photoset
          &:nth-child(3)
            grid-row 2
            grid-column 1/-1
            -ms-grid-row 3
            -ms-grid-column 1
            -ms-grid-column-span 3
            -ms-grid-row-span 1
          &:nth-child(4)
            -ms-grid-row 5
            -ms-grid-column 1
          &:nth-child(5)
            -ms-grid-row 5
            -ms-grid-column 3
  .footer
    &_block
      display flex
      flex-direction column
      align-items center
      text-align center
      padding-top 0
    .logo
      margin 40px 20px
      &_name
        margin-left -20px
      &:before
        width 100%
      &_type
        margin-left 0
    .contacts
      text-align center
@media all and (max-width w_xl)
  header .header
    &_block
      grid-template-rows auto auto auto
      -ms-grid-rows auto w_gatter auto w_gatter auto
      > div:nth-child(1)
        -ms-grid-row 1
        -ms-grid-column 1
        -ms-grid-column-span 3
        grid-row 1
        grid-column 1/-1
        text-align center
      > .logo
        -ms-grid-row 5
        -ms-grid-column 1
        -ms-grid-column-span 3
        grid-row 3
        grid-column 1/-1
        width 100%
      > div:nth-child(3)
        -ms-grid-row 3
        -ms-grid-column 1
        -ms-grid-column-span 3
        grid-row 2
        grid-column 1/-1
        text-align center

  #main.main
    .flexslider
      .slider
        &_content
          &:before
            height 130px
          &__headline
            font-size 20px
            letter-spacing 3px
            margin-top 17px
            &:before
              bottom 10px
              width 70px
              height 1px
            &:after
              top 10px
              width 70px
              height 1px
          &__btn
            margin-top 20px
    .introduction
      &_content
        &__text
          padding 20px
      &--chef
        .chef_text
          padding-top 20px
          &__btn
            margin-top 20px
    .about-us_text__btn
      padding 10px 15px
      margin-top 25px
    .menu
      &-items
        grid-template-columns 1fr
        -ms-grid-columns 1fr
        -ms-grid-rows auto w_gatter auto w_gatter auto w_gatter auto
        > div:nth-child(1)
          -ms-grid-row 1
          -ms-grid-column 1
        > div:nth-child(2)
          -ms-grid-row 3
          -ms-grid-column 1
        > div:nth-child(3)
          -ms-grid-row 5
          -ms-grid-column 1
        > div:nth-child(4)
          -ms-grid-row 7
          -ms-grid-column 1
        &_item__img
          background-size cover
      &_other
        font-size 16px
    .event
      .plans-wrap
        grid-template-columns 1fr
        -ms-grid-columns 1fr
        -ms-grid-row 1fr 1fr
        &_center
          display none
        .plan
          &_number
            font-size 30px
        &_left
          -ms-grid-row 1
          -ms-grid-column 1
          -ms-grid-column-span 1
          .plan
            padding-right 0
            &_headline
              padding-right 50px
        &_right
          -ms-grid-row 2
          -ms-grid-column 1
          -ms-grid-column-span 1
          .plan
            padding-left 0
            &_headline
              padding-left  50px
    .gallery-block
      .gallery
        grid-template-columns 1fr
        grid-template-rows  250px 250px 250px 250px 250px
        -ms-grid-columns 1fr
        -ms-grid-rows 250px w_gatter 250px w_gatter 250px w_gatter 250px w_gatter 250px
        &_photoset
          &:nth-child(1)
            -ms-grid-row 1
            -ms-grid-column 1
          &:nth-child(2)
            -ms-grid-row 3
            -ms-grid-column 1
          &:nth-child(3)
            grid-row 3
            grid-column 1
            -ms-grid-row 5
            -ms-grid-column 1
            -ms-grid-column-span 1
          &:nth-child(4)
            -ms-grid-row 7
            -ms-grid-column 1
          &:nth-child(5)
            -ms-grid-row 9
            -ms-grid-column 1
    .book
      .headline
        &_text
          font-size 30px
        &:after
          top 20px
          height 1px
    .feadback
      &_form
        display flex
        flex-direction column
        > div
          margin-bottom w_gatter
        > button
           margin-top 10px
      &_circle-btn
        bottom -14px
        width 116px
        height 43px
        padding-bottom 42px
        &:before
          top 10px
        &__text
          bottom 0
          font-size 12px
@media all and (min-width w_biggest)
  .introduction
    &--about-us
      &:after
        background-size contain
