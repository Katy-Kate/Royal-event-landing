      font-size 12px
      text-align center
      text-transform uppercase
      letter-spacing 1.8px
      color c_acc-cont
  .cart
    width 170px
    text-transform uppercase
    color c_acc-cont
    .fas:before
      margin-right 12px
      font-size 11px
