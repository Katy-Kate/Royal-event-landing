@media all and (max-width w_sm)
  .header
    &_nav
      display none
      &__item
        padding 10px 10px
    .mobile-menu
      position absolute
      top 0
      right -100%
      z-index 9998
      display block!important
      max-width 500px
      width 100%
      height 100vh
      background #fff
      transition 0.6s ease
      opacity 0
      ul
        position relative
        top 50%
        padding 0
        transform translateY(-50%)
        li
          list-style: none
          a
            position relative
            display block
            width 100%
            margin 0 auto
            text-align center
            text-decoration none
            font-size 21px
            line-height 39px
            text-transform uppercase
            overflow hidden
            &:after
              position absolute
              right -100%
              top 0
              z-index -1
              width 100%
              height 100%
              content ''
              background c_gray-text
              transition 0.4s ease
            &:hover
              color #ffffff
              background c_acc
              &:after
                right 0

      &--open
        right 0
        opacity 1
    .mobile-nav-button
      position absolute
      right 0
      top 0
      z-index 9999
      display inherit!important
      width 35px
      height 30px
      margin 1rem 2rem
      cursor pointer
      &__line
        position relative
        width 100%
        height 1px
        background c_grey
        transition 1s ease
        &:nth-of-type(2)
          margin 0.5rem 0
        &--1
          position absolute
          top 13px
          transform rotate(45deg)
        &--2
          display none
        &--3
          position absolute
          top 13px
          transform rotate(135deg)
    &_block
      align-items center
      display grid
      display -ms-grid
      grid-template-columns 1fr 1fr
      grid-template-rows 1fr 1fr
      -ms-grid-columns 1fr w_gatter 1fr
      -ms-grid-rows 1fr w_gatter 1fr
      grid-gap w_gatter
      > div:nth-child(1)
