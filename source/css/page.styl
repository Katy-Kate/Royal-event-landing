.header
  height auto
  .preloader
    position fixed
    left 0
    top 0
    width 100%
    height 100%
    z-index 999999
    text-align center
    background #fff
    background-image url(../images/preloader.gif)
    background-position center center
    background-repeat no-repeat

  &_nav
    display flex
    justify-content center
    padding 20px 0
    background c_main
    &__item
      width auto
      padding 0 25px
      border-right 1px solid
      font-size 14px
      letter-spacing 1.7px
      line-height 1.2
      text-transform uppercase
      color c_acc-cont
      &--active
        color c_acc
        &:before
          color c_acc
      &:last-of-type
        border-color transparent
    .fas
      font-size 12px
  &_block
    display flex
    justify-content space-between
    align-content center
    padding-top 55px
    padding-bottom 13px
    max_width()
  .contacts
    width 200px
    padding-top 3px
    text-align right
    font-weight bold
    color c_acc-cont
    &_tel
      margin-left 7px
      letter-spacing 1.4px
    .fa-phone
      transform rotate(90deg)
      color c_acc-cont
      font-size 11px
      line-height 1
    &_mail
      display block
      font-size 14px
      line-height 1.7
      letter-spacing 1.4px
      text-transform uppercase
  .logo
    position relative
    z-index 2
    display block
    width 305px
    &:before
      content ""
      position absolute
      top 0
      left 0
      width 274px
      height 85px
      background url(../images/logo.png) no-repeat center
      background-size contain
      z-index 3
    &_name
      margin-left -44px
      font-family f_acc
      font-size 45px
      line-height .7
      text-align center
      color c_cont
      letter-spacing 0
      &__bottom
        margin-left 17px
        letter-spacing 0
        line-height .3
    &_type
      margin-top 22px
